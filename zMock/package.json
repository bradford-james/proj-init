{
  "name": "zMock",
  "version": "1.0.0",
  "description": "./README.md",
  "main": "index.js",
  "scripts": {
    "start": "node bin/project-init.js",
    "dev": "env NODE_ENV=dev node bin/project-init.js",
    "reset": "node src/core/utils/reset.js local",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand  --watch",
    "format": "npm run prettier -- --write",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "commit": "npm run format && npm run lint && npm run test && git add . && git cz",
    "release": "git push --follow-tags",
    "semantic-release": "semantic-release"
  },
  "keywords": [],
  "author": "Bradford James <bradwettig@gmail.com>",
  "license": "MIT",
  "bin": {
    "zMock": "bin/zMock.js"
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^1.19.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "cz-conventional-changelog": "^3.1.0",
    "semantic-release": "^17.0.2",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.3",
    "@semantic-release/npm": "^7.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "",
      "prepare-commit-msg": "",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-commit": "",
      "pre-push": ""
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
